{
  "name": "antd-vue-components",
  "version": "1.0.2",
  "main": "./dist/cjs/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/index.css": {
      "import": "./dist/esm/index.css",
      "require": "./dist/cjs/index.css"
    }
  },
  "types": "./dist/types/components",
  "keywords": [
    "ant-design-vue",
    "vue3"
  ],
  "files": [
    "dist"
  ],
  "author": {
    "name": "lxjyo",
    "url": "https://github.com/lxjyo"
  },
  "description": "基于ant-design-vue组件的二次封装组件",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "rollup --config rollup.config.mjs",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "cross-env HUSKY_SKIP_HOOKS=1 standard-version",
    "release-minor": "cross-env HUSKY_SKIP_HOOKS=1 standard-version -r minor",
    "release-major": "cross-env HUSKY_SKIP_HOOKS=1 standard-version -r major",
    "release-patch": "cross-env HUSKY_SKIP_HOOKS=1 standard-version -r patch"
  },
  "lint-staged": {
    "**/*.{vue,ts,js}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@chromatic-com/storybook": "^1.9.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "ant-design-vue": "^4.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.6",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "rollup": "^4.21.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-resolve": "0.0.1-predev.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "standard-version": "^9.5.0",
    "storybook": "^8.2.9",
    "typescript": "~5.4.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue": "^3.2.25",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "ant-design-vue": "^4.0.0",
    "vue": "^3.2.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lxjyo/vue-components.git"
  },
  "engines": {
    "node": ">=18"
  }
}